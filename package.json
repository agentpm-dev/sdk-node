{
  "name": "@agentpm/sdk",
  "version": "0.1.1",
  "description": "AgentPM Node SDK",
  "license": "MIT",
  "author": "AgentPM",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentpm-dev/sdk-node.git"
  },
  "bugs": {
    "url": "https://github.com/agentpm-dev/sdk-node/issues"
  },
  "homepage": "https://agentpackagemanager.com/",
  "keywords": [
    "agentpm",
    "agents",
    "tools",
    "sdk"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.15.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "prepublishOnly": "pnpm build && pnpm test && pnpm lint && pnpm format"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@langchain/core": ">=0.2.0"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@langchain/core": "^0.2.0",
    "@types/node": "^24.3.0",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  }
}
